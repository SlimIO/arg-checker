{
    "name": "@slimio/arg-checker",
    "version": "0.2.1",
    "description": "SlimIO Argument Checker",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && nyc --reporter=lcov ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/arg-checker.git"
    },
    "keywords": [
        "arg",
        "checker",
        "validation",
        "validator",
        "predicate"
    ],
    "files": [
        "index.js",
        "index.d.ts",
        "src/isValidIdentifier.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/arg-checker/issues"
    },
    "homepage": "https://github.com/SlimIO/arg-checker#readme",
    "dependencies": {
        "callsites": "^3.1.0",
        "clean-stack": "^2.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.0.0",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.0.0",
        "@slimio/is": "^1.5.1",
        "@slimio/psp": "^0.11.1",
        "ava": "^3.8.2",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "jsdoc": "^3.6.4",
        "nyc": "^15.0.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/isValidIdentifier.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 50,
        "statements": 50,
        "functions": 50,
        "branches": 50
    }
}
